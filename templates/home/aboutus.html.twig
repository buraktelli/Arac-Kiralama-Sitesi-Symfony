{% extends 'homebase.html.twig' %}

{% block title %}About Us | {{data[0].title}}{% endblock %}
{% block keywords %}{{data[0].keywords}}{% endblock %}
{% block description %}{{data[0].description}}{% endblock %}

{% block body %}
    <!-- Subhead
    ================================================== -->
    <section id="subintro">
        <div class="jumbotron subhead" id="overview">
            <div class="container">
                <div class="row">
                    <div class="span12">
                        <div class="centered">
                            <h3>About us</h3>
                            <p>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="breadcrumb">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <ul class="breadcrumb notop">
                        <li><a href="/">Home</a><span class="divider">/</span></li>
                        <li class="active">About us</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section id="maincontent">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <div class="well">
                        <div class="centered">
                            <i class="icon-circled icon-64 icon-suitcase active"></i>
                            <h4>About Us</h4>
                            <div class="dotted_line">
                            </div>
                            <p>
                                {{ data[0].aboutus | raw}}
                            </p>
                        </div>
                    </div>
                </div>

            </div>
            <div class="row">
                <div class="span12">
                    <div class="divider">
                    </div>
                </div>
            </div>

        </div>
    </section>

{% endblock %}

